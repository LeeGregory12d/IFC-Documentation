        <TypePropertySingleValue>
          <DataType />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases />
      <DefinitionAliases />
    </PropertyDef>
    <PropertyDef ifdguid="381428b1c91c4705a6c85bc231b06ae4">
